<%= f.label :Profesion %>
<span id="form4"> <%= f.text_field :profession, style: 'float: left; width:90%; display:block' %><br><br></span>
<%= f.label :Email %>
<span id="form5"> <%= f.email_field :email, style: 'float: left; width:90%; display:block' %><br><br></span>
<%= f.label :Description %>
<span id="form6"> <%= f.text_area :description, style: 'float: left; width:90%; display:block' %><br><br></span>
<% end %>
</div>
