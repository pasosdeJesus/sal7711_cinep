<h1>Formulario</h1>
<%= form_for :picture, url: pictures_path do |f| %>
 <%= f.label :Nombre %>
<span id="form1"> <%= f.text_field :name, style: 'float: left; width: 90%; display:block' %><br><br></span>
<%= f.label :Apellido %>
<span id="form2"> <%= f.text_field :last_name, style: 'float: left; width:90%; display:block' %><br><br></span>
<%= f.label :Edad %>
